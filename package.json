{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "description": "react + redux + ts bolierplate",
  "homepage": "https://github.com/CatalystDP/react-ts-boilerplate",
  "keywords": [
    "react",
    "redux",
    "typescript",
    "boilerplate"
  ],
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run clean && cross-env process.env.TS_NODE_PROJECT=tsconfig.scripts.json ts-node scripts/server.ts",
    "build": "npm run clean && cross-env process.env.TS_NODE_PROJECT=tsconfig.scripts.json  ts-node  ./node_modules/.bin/webpack --config scripts/webpack.prod.conf.ts -p --progress --colors",
    "format": "prettier --write \"src/**/*.{ts,js,tsx,css}\"",
    "clean": "rimraf ./dist",
    "init:proj": "rimraf .git",
    "commit": "npm run format && git add . && git cz",
    "release": "standard-version"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/classnames": "^2.2.7",
    "@types/history": "^4.7.2",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.0",
    "@types/react-router": "^4.4.3",
    "@types/redux-actions": "2.3.1",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.23",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-env": "1.13.6",
    "@types/webpack-merge": "^4.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "commitizen": "^3.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "ejs-loader": "^0.3.1",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^4.0.0-alpha",
    "mini-css-extract-plugin": "^0.5.0",
    "postcss": "^7.0.13",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "postcss-reporter": "^6.0.1",
    "postcss-url": "^8.0.0",
    "prettier": "^1.15.3",
    "redux-devtools-extension": "^2.13.7",
    "rimraf": "^2.6.3",
    "standard-version": "^4.4.0",
    "style-loader": "^0.23.1",
    "ts-import-plugin": "^1.5.5",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.4",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "write-file-webpack-plugin": "^4.5.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "classnames": "^2.2.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-thunk": "^2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
